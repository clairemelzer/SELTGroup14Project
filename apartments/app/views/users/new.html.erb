<h1> Sign Up </h1>xe

<%= form_tag("/users") do %>

  <%= label_tag(:name, "Name") %>
  <%= text_field_tag(:name)%>
  
  <%= label_tag(:email, "Email") %>
  <%= text_field_tag(:email)%>
  
  <%= label_tag(:password, "Password") %>
  <%= text_field_tag(:password)%>
    
  <%= submit_tag("Create User") %>
<% end %>